import java.util.Scanner;
class cls1
{
    void add(int p,int q)
    {
        System.out.println(p+q);
    }
}
class cls2 extends cls1 {
    void mul(int p, int q) { // renamed from mult to mul
        System.out.println(p * q);
    }
    void add(int p, int q) { // overriding add to do sum of squares
        System.out.println(p + q);
    }
    void task(int p, int q) {
        System.out.println(p*p + q*q); // sum of squares
        }
}
public class W03_P5{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
    
        cls2 obj=new cls2();
        int a=sc.nextInt();
        int b=sc.nextInt();
        //String tilde=sc.next();
        obj.add(a,b);
        obj.mul(a,b);
        obj.task(a,b);
    
    }
}
